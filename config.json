{
  "sharePath": "./shared",
  "maxFileSize": 100000000,
  "permissions": {
    "upload": true,
    "delete": true,
    "download": true,
    "rename": true,
    "createFolder": true,
    "move": true,
    "dragAndDrop": true
  },
  "excludedFiles": [
    ".git",
    ".env",
    "node_modules",
    "config.json"
  ],
  "allowedFileTypes": [
    "image/*",
    "application/pdf",
    "text/*",
    "application/msword",
    "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
    "application/vnd.ms-excel",
    "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
  ],
  "thumbnailSize": {
    "width": 200,
    "height": 200
  },
  "cacheDuration": 3600
} 